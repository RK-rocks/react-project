{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "0.9.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "redux-devtools": "^3.5.0"
  },
  "scripts": {
    "build-css": "node-sass src/assets/scss/ -o src/assets/css/",
    "watch-css": "npm run build-css && node-sass src/assets/scss/ -o src/assets/css/ --watch",
    "start": "npm-run-all watch-css start-js",
    "start-js": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
